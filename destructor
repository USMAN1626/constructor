#include<iostream>
using namespace std;
class rectangle
{
    int length;
    int width;
public:
    rectangle();
    rectangle(int , int );
    int area();
    
};
rectangle:: rectangle()
{
    length=4;
    width=10;
    cout<<" HEY I AM  default constructor "<<endl;
}
rectangle:: rectangle(int a , int b)
    {
    width=a;
    length=b;
    cout<<" HEY I AM  PARAMETRIZED CONSTRUCTOR "<<endl;
    }
   int rectangle::area()
    {
        return length * width ;
       
    }
int main()
{
    rectangle r1;
    rectangle r2(3,6);
    cout<<" THE AREA OF RTHE RECTANGLE IS "<<r1.area()<<endl;
}



